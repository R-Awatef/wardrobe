
      <div class="bg-white rounded p-3 shadow">
        <h3>Update outfit style</h3>
        <%= simple_form_for @outfit do |f| %>
          <%= f.input :title, placeholder: 'The casual' %>
          <%= f.input :photo, as: :file %>
          <%= f.submit class: 'btn btn-dark' %>
        <% end %>
      </div>
